    "typescript",
    "form",
    "validator",
    "formdata",
    "urlsearchparams",
    "sveltekit"
  ],
  "homepage": "https://github.com/gauben/formgator#readme",
  "bugs": "https://github.com/gauben/formgator/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gauben/formgator.git"
  },
  "license": "MIT",
  "author": "Gautier Ben AÃ¯m (https://gautier.dev)",
  "sideEffects": false,
  "type": "module",
  "exports": {
